						.result
							.imgbox
								img(src="./img/tools/result_image.jpg", alt="")
							.flag
								img(src="./img/tools/result_flag.png", alt="")
							.name
								h5 Vietnam
							.rank
								h5 Ranked 87th in 2019
								a.view(href="#") View global ranking
							.list
								h5 51 destinations visa-free access
								a(href="#") Hide list
							.social
								a.fab.fa-facebook-f(href="#")
								a.fab.fa-twitter(href="#")
								a.fab.fa-linkedin-in(href="#")
								span.mdi.mdi-close
