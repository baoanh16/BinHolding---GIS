mixin breadcrumb(breadcrumb)
	.breadcrumb-wrapper
		.container
			ul
				each item in breadcrumb
					li
						a(href="#")= item
mixin PageBannerWrapper(link)
	.banner-wrapper
		a(href="#")
			img(src="./img/banner/"+link+".jpg", alt="")

.page-banner
	+PageBannerWrapper(link)
	+breadcrumb(breadcrumb)
